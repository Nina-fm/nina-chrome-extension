{
  "manifest_version": 2,

  "name": "Nina.fm",
  "description": "This extension allows the user to listen Nina.fm during the web browsing.",
  "version": "1.0",

  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "equalizer/stepoff.png",
    "default_title": "Nina.fm"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
